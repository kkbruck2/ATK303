jQuery( function( $ ) {
	// Helper function: Count the number of selected checkboxes of a given type
	// in the current list table
	function countCheckedInputs( selector ) {
		return $( '#the-list input[type=checkbox]' + selector ).filter( ':checked' ).length;
	}

	// Helper function: Get the value of a dropdown box in the bulk edit screen
	// for posts (ignore missing inputs or "no change" value=-1)
	function getDropdownValue( name ) {
		var value = $( '#bulk-edit select[name=' + name + ']' ).val();
		if ( ! value || value === '-1' ) {
			return '';
		}
		return value;
	}

	$( '#bulk-action-selector-top, #bulk-action-selector-bottom' ).each( function() {
		var $this = $( this );
		var which = $this.prop( 'id' ).replace( /^bulk-action-selector-/, '' );

		// Record an event when a new option is selected from the dropdown
		$this.on( 'change', function() {
			var value = $this.val();
			window._tkq = window._tkq || [];
			window._tkq.push( [
				'recordEvent',
				'wpadmin_bulk_edit_option_select',
				{
					value: value,
					location: which,
					screen: window.wpcomUsageStats.screenId,
				}
			] );
		} );

		// Record an event when an option is chosen using the Apply button
		$this.next( 'input[type=submit][id^=doaction]' ).on( 'click', function() {
			var value = $this.val();
			window._tkq = window._tkq || [];
			window._tkq.push( [
				'recordEvent',
				'wpadmin_bulk_edit_option_apply',
				{
					value: value,
					location: which,
					screen: window.wpcomUsageStats.screenId,
					selected_items_count: (
						// Users list checkboxes
						countCheckedInputs( '[id^=user_]' ) +
						// Everything else
						countCheckedInputs( '[id^=cb-select-]' )
					),
				}
			] );
		} );
	} );

	// Record an event when a bulk edit request is sent for a given post type
	$( '#bulk_edit' ).on( 'click', function() {
		var tagsValue = $( '#bulk-edit textarea.tax_input_post_tag' ).val();
		window._tkq = window._tkq || [];
		window._tkq.push( [
			'recordEvent',
			'wpadmin_bulk_edit_posts_apply',
			{
				screen: window.wpcomUsageStats.screenId, // contains the post type
				edit_items_count: $( '#bulk-titles' ).children().length,
				edit_category_count: countCheckedInputs( '[id^=in-category-]' ),
				edit_tag_count: tagsValue ? tagsValue.split( ',' ).length : 0,
				// Dropdown boxes (all possible; not all are present on a single screen)
				edit_author_value:          getDropdownValue( 'post_author' ),
				edit_page_template_value:   getDropdownValue( 'page_template' ),
				edit_page_parent_value:     getDropdownValue( 'page_parent' ),
				edit_comment_status_value:  getDropdownValue( 'comment_status' ),
				edit_status_value:          getDropdownValue( '_status' ),
				edit_post_format_value:     getDropdownValue( 'post_format' ),
				edit_pingback_status_value: getDropdownValue( 'ping_status' ),
				edit_sticky_value:          getDropdownValue( 'sticky' ),
			},
		] );
	} );
} );
